@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>


@{
    ViewData["Title"] = "RoleList";
}

<h1 class="text-center font-weight-bold">Manage account</h1>
<h3 class="font-weight-bold">Roles</h3>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Actions</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
                <td><a class="btn btn-primary" asp-area="" asp-controller="Admins" asp-action="UserList">Add Role</a></td>
            </tr>

        }
    </tbody>
</table>

